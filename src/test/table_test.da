
        ;; create object and bind it to self
        NEWOBJECT    r1
        SETSELF      r1

        ;; self.h = 'ads'
        LITERAL      r2   "ads"
        SET          r2   "h"

        ;; null all registers
        MOVE         r1   r0
        MOVE         r2   r0

        ;; r3 = self.h
        GET          r3   "h"

        ;; halt
        RET          r0   r0
